{
  "name": "mapd-frontend-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate - React / Redux / Sass / Webpack / Babel / Typescript / Jest",
  "main": "src/index.tsx",
  "scripts": {
    "build": "webpack --colors --progress --display-error-details --config webpack.config.prod.js",
    "build:dev": "webpack --colors --progress --display-error-details --config webpack.config.dev.js",
    "lint": "eslint . --ext=js --ext=jsx --ext=ts --ext=tsx --ext=json",
    "start": "webpack-serve --config webpack.config.dev.js --open",
    "start:prod": "npm run build && http-server ./dist",
    "test": "jest --config jest.config.json",
    "ts": "tsc --pretty"
  },
  "author": "Jonathan Rajavuori <jonathan.rajavuori@mapd.com>",
  "license": "MIT",
